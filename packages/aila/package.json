    "test-coverage": "COLLECT_TEST_COVERAGE=true pnpm with-env jest --colors --config jest.config.mjs --coverage",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@oakai/prettier-config",
  "eslintConfig": {
    "extends": "@oakai/eslint-config",
