      "project": "./tsconfig.json"
    },
    "rules": {}
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.55",
    "@clerk/nextjs": "5.7.2",
    "@elastic/elasticsearch": "^8.13.1",
    "@oakai/core": "*",
    "@oakai/db": "*",
    "@oakai/exports": "*",
    "@oakai/logger": "*",
    "@sentry/nextjs": "^8.35.0",
    "@vercel/kv": "^0.2.2",
    "ai": "^4.0.20",
    "american-british-english-translator": "^0.2.1",
    "cloudinary": "^1.41.1",
    "dedent": "^1.5.3",
    "dotenv-cli": "^6.0.0",
    "jsonrepair": "^3.8.0",
