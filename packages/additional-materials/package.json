{
  "name": "@oakai/additional-materials",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "test": "pnpm with-env jest --colors --config jest.config.mjs",
    "test-coverage": "COLLECT_TEST_COVERAGE=true pnpm with-env jest --colors --config jest.config.mjs --coverage",
    "test-file": "pnpm with-env jest --config jest.config.mjs --runTestsByPath TEST_FILEPATH",
    "test:seq": "pnpm with-env jest --colors --config jest.config.mjs --verbose --runInBand",
    "type-check": "tsc --noEmit",
    "with-env": "dotenv -e ../../.env --"
  },
  "prettier": "@oakai/prettier-config",
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@oakai/eslint-config"
  },
  "dependencies": {
    "@ai-sdk/openai": "^0.0.55",
    "@clerk/nextjs": "5.7.2",
    "@oakai/logger": "*",
    "@sentry/nextjs": "^8.49.0",
    "ai": "^4.1.00",
    "dotenv-cli": "^6.0.0",
    "openai": "^4.58.1",
    "superjson": "^1.9.1",
    "tiny-invariant": "^1.3.1",
    "zod": "3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@oakai/eslint-config": "*",
    "@oakai/prettier-config": "*",
    "@types/dedent": "^0.7.2",
    "ts-jest": "^29.2.5",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=8"
  }
}
