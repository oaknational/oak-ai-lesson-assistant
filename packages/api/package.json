{
  "name": "@oakai/api",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "main": "./index.ts",
  "types": "./index.ts",
  "scripts": {
    "check": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules",
    "lint": "eslint .",
    "test": "jest --config jest.config.mjs",
    "test:coverage": "COLLECT_TEST_COVERAGE=true jest --config jest.config.mjs --coverage",
    "test:file": "jest --config jest.config.mjs --runTestsByPath TEST_FILEPATH",
    "test:seq": "jest --config jest.config.mjs --verbose --runInBand",
    "type-check": "tsc --noEmit"
  },
  "prettier": "@oakai/prettier-config",
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@oakai/eslint-config"
  },
  "dependencies": {
    "@clerk/nextjs": "5.7.2",
    "@oakai/aila": "*",
    "@oakai/core": "*",
    "@oakai/db": "*",
    "@oakai/exports": "*",
    "@oakai/logger": "*",
    "@oakai/teaching-materials": "*",
    "@trpc/server": "10.45.2",
    "@vercel/kv": "^0.2.2",
    "cloudinary": "^1.41.1",
    "next": "14.2.33",
    "remeda": "^1.29.0",
    "superjson": "^1.9.1",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@oakai/eslint-config": "*",
    "@oakai/prettier-config": "*",
    "@types/archiver": "^6.0.3",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=20.9.0",
    "pnpm": ">=8"
  }
}
