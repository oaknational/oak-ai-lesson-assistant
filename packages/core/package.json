{
  "name": "@oakai/core",
  "version": "1.0.0",
  "description": "Core functionality for the project",
  "license": "MIT",
  "author": "",
  "type": "module",
  "main": "./index.ts",
  "scripts": {
    "admin": "open http://localhost:8288",
    "check": "tsc --noEmit",
    "clean": "rm -rf .turbo node_modules",
    "debug": "open https://localhost:2525/api/inngest",
    "dev": "pnpm exec inngest-cli dev -u http://localhost:2525/api/inngest",
    "doppler:run:dev": "doppler run -c dev --silent",
    "doppler:run:prd": "doppler run -c prd --silent",
    "doppler:run:stg": "doppler run -c stg --silent",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "process:answersAndDistractors": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} ./src/scripts/batchAnswersAndDistractorsForJudgement.ts",
    "process:createSnippetsForNewLessons": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} ./src/scripts/import-new-lessons/createSnippetsForNewLessons.ts",
    "process:getTranscriptsForNewLessons": "ts-node --compilerOptions \"{\\\"module\\\": \\\"CommonJS\\\", \\\"allowJs\\\": true}\" ./src/scripts/import-new-lessons/getTranscriptsForNewLessons.ts",
    "process:importNewLessons": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} ./src/scripts/import-new-lessons/importNewLessons.ts",
    "process:lessons": "pnpm doppler:run:dev -- ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} ./src/scripts/processLessons.ts",
    "process:lessons:dev": "pnpm doppler:run:dev -- pnpm process:lessons",
    "prompts": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} ./src/scripts/setupPrompts.ts",
    "prompts:dev": "dotenv -e ../../.env -- ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} ./src/scripts/setupPrompts.ts",
    "seed": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} ./src/scripts/seedLessons.ts",
    "seed:dev": "dotenv -e ../../.env -- pnpm seed",
    "seed:prd": "pnpm doppler:run:prd -- pnpm seed",
    "seed:stg": "pnpm doppler:run:stg -- pnpm seed",
    "type-check": "tsc --noEmit"
  },
  "prettier": "@oakai/prettier-config",
  "eslintConfig": {
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": "@oakai/eslint-config"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@googleapis/docs": "^5.0.1",
    "@googleapis/drive": "^8.7.0",
    "@googleapis/slides": "^1.0.5",
    "@hubspot/api-client": "^11.2.0",
    "@oakai/db": "*",
    "@oakai/logger": "*",
    "@slack/web-api": "^7.10.0",
    "@types/json-stringify-safe": "^5.0.3",
    "@upstash/ratelimit": "^2.0.1",
    "@upstash/redis": "^1.22.0",
    "@vercel/functions": "^1.4.0",
    "cloudinary": "^1.41.1",
    "dd-trace": "^5.21.0",
    "google-auth-library": "^9.7.0",
    "inngest": "^3.16.1",
    "json-stringify-safe": "^5.0.1",
    "langchain": "^0.0.184",
    "obscenity": "^0.1.4",
    "openai": "^4.96.0",
    "tiny-invariant": "^1.3.1",
    "ts-dedent": "^2.2.0",
    "ts-md5": "^1.3.1",
    "ts-node": "^10.9.2",
    "untruncate-json": "^0.0.1",
    "webvtt-parser": "^2.2.0",
    "zod": "3.23.8",
    "zod-to-json-schema": "^3.23.0"
  },
  "devDependencies": {
    "@oakai/prettier-config": "*",
    "inngest-cli": "^0.29.0"
  }
}
